{
  "expo": {
    "name": "instagram",
    "slug": "instagram",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo/Mirrorgram-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/open-mirrorgram.png",
      "resizeMode": "contain",
      "backgroundColor": "white"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo/Mirrorgram-icon.png",
        "backgroundColor": "#000"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "mirrorgram"
  }
}
